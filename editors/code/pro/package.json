{
  "name": "git-conventional-commit-ls-base",
  "version": "1.0.0",
  "description": "Highlighting, linting, and completion for conventional commit messages",
  "engines": {
    "vscode": "^1.77.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skalt/conventional-commit-language-server.git",
    "directory": "editors/code/pro"
  },
  "main": "./dist/main.min.common.js",
  "keywords": [
    "git",
    "git-commit",
    "language-client",
    "language-server",
    "lsp",
    "vscode-extension"
  ],
  "categories": [
    "Programming Languages"
  ],
  "extensionKind": [
    "workspace"
  ],
  "contributes": {
    "languages": [
      {
        "id": "git-commit",
        "filenames": [
          "COMMIT_EDITMSG"
        ]
      }
    ],
    "grammars": [
      {
        "language": "git-commit",
        "scopeName": "source.git-commit",
        "path": "./src/tmLanguage.json"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:git-commit"
  ],
  "author": "Steven Kalt <steven@kalt.cloud>",
  "homepage": "https://github.com/skalt/conventional-commit-language-server#TODO",
  "license": "SEE LICENSE IN ./LICENSE.md",
  "devDependencies": {
    "@types/node": "^18.17.11",
    "@types/vscode": "~1.77.0",
    "@vscode/vsce": "~2.20.1",
    "esbuild": "~0.17.19",
    "js-yaml": "^4.1.0",
    "prettier": "^2.8.8",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "vscode-languageclient": "^8.1.0"
  }
}
